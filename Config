# 导入必要的包

pip install -i https://mirrors.aliyun.com/pypi/simple/ opencv-python==4.5.5.62
pip install -i https://mirrors.aliyun.com/pypi/simple/ opencv-contrib-python==4.5.5.62
pip install -i https://mirrors.aliyun.com/pypi/simple/ argparse
pip install -i https://mirrors.aliyun.com/pypi/simple/ imutils
pip install  -i https://mirrors.aliyun.com/pypi/simple/ pyserial
pip install -i https://mirrors.aliyun.com/pypi/simple/ pynput
pip install -i https://mirrors.aliyun.com/pypi/simple/ kb_upper
pip install pymycobot
pip install -i https://mirrors.aliyun.com/pypi/simple/ pupil-apriltags
pip install -i https://mirrors.aliyun.com/pypi/simple/ redis

pip install -i https://mirrors.aliyun.com/pypi/simple/ pyzbar

pip install -i https://mirrors.aliyun.com/pypi/simple/ apriltag
pip install airtest -i https://pypi.tuna.tsinghua.edu.cn/simple
pip install appium-python-client==2.11.0 -i https://mirrors.aliyun.com/pypi/simple